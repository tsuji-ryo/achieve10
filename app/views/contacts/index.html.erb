<div class="container">
  <div class="wrapper col-md-8 col-md-offset-2 col-sm-10">
    <h1>お問い合わせ一覧</h1>

    <% @contacts.each do |contact| %>
      <p>名前:<%= contact.name %></p>
      <p>メールアドレス:<%= contact.email %></p>
      <p>お問い合わせ:<%= contact.content %></p>
      <%= link_to "お問い合わせをする", new_contact_path, class: 'btn btn-default' %>
      <% end -%>

    <%= link_to "ブログを作成する", new_blog_path, class: 'btn btn-default' %>
  </div>
</div>